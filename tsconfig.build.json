{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "noEmit": false /* Do not emit outputs. */,
        "importHelpers": true /* Import emit helpers from 'tslib'. */,
        "noEmitHelpers": true,
        "module": "es2022",
        "moduleResolution": "node",
        "skipLibCheck": true
    },
    "exclude": [
        "dist",
        "**/*test.ts",
        "**/fixtures/*"
    ]
}
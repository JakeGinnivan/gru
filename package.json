{
    "name": "gru",
    "version": "1.0.0",
    "main": "dist/cjs/index.js",
    "module": "dist/es2015/index.js",
    "license": "MIT",
    "scripts": {
        "build": "yarn build:es2015 && yarn build:cjs",
        "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015 --noEmit false",
        "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs --noEmit false",
        "pretest": "tsc -p src/fixtures",
        "test": "yarn jest",
        "lint": "eslint --ext .js,.ts src"
    },
    "devDependencies": {
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jest": "^24.0.16",
        "@types/node": "^10.14.14",
        "eslint-config-wanews-base": "^1.0.0",
        "isomorphic-fetch": "^2.2.1",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "tslib": "^1.10.0",
        "typescript": "^3.5.3",
        "typescript-log": "^1.1.0"
    }
}